
type Mutation {
  removeContacts(contactsId: ID!): Boolean!
}

type Query {
  contacts(first: Int! = 100, after: String!): ContactsConnection!
  addedMe(userId: ID!): Boolean!
}

type ContactsConnection {
  totalCount: Int!
  edges: [Edge!]
  pageInfo: PageInfo!
}

type Edge{
  node: Contacts!
  cursor: String
}

type Contacts {
  id: ID!
  remarkName: String!
}

type PageInfo{
  endCursor: String
  hasNextPage: Boolean!
}
