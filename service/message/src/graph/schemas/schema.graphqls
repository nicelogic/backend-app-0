type Query {
  getChats(UserId: String!): [Chat!]
  getChat(chatId: String!): Chat!
}

type Mutation {
  createChat(UserId: String!, memberIds: [String!]!, name: String!): Chat!
  deleteChat(chatId: String!): String!
}

type Chat {
  id: String!
  members: [User!]!
  messages: [Message]
  name: String!
  lastMessage: Message
}

type User {
  id: String!
  name: String
}
